{{#each menus}}


<div class="col-md-12 item {{#if @first}}active{{/if}} " data-id="{{id}}">

    <img style="display:none" src="reportMemo.do?method=getImage&reportMemoId={{id}}&ds={{getCurrentTime}}">


    <div class="col-md-8 col-xs-8">
        <div class="center-block img" data-id="{{id}}" data-menuId="{{keyValue}}" style="height:940px;background-size: contain;background-image:url('reportMemo.do?method=getImage&reportMemoId={{id}}&ds={{getCurrentTime}}');background-repeat:no-repeat;background-position:top center">


        </div>
    </div>


    <div class="col-md-4 col-xs-4 column memo">


		<pre id="memoBy" class="memo2"><span style="font-size: 15px">{{memoBy}}<br>{{keyDate}}</span></pre>
        <pre id="reportMemo" class="memo2"><span style="font-size: 15px">{{memo}}</span></pre>

    </div>

</div>
{{/each}}
